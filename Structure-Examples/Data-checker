
#include <stdio.h>
#include <string.h>
struct date
{
    int day;
    int month;
    int year;
};
int main()
{
    struct date day,month,year;
    printf("Enter day:");
    scanf("%d",&day.day);
    printf("Enter month: ");
    scanf("%d",&month.month);
    printf("Enter year: ");
    scanf("%d",&year.year);
    if(year.year % 400 == 0)
    {
        if(month.month == 2 && day.day <= 29 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 8 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 1 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 3 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 4 && day.day <= 30 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 5 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 6 && day.day <= 30 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 7 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 9 && day.day <= 30 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 10 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 11 && day.day <= 30 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 12 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else
        {
            printf("invalid date");
        }
    }
    else
    {
        if(month.month == 2 && day.day <= 28 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 8 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 1 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 3 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 4 && day.day <= 30 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 5 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 6 && day.day <= 30 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 7 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 9 && day.day <= 30 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 10 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 11 && day.day <= 30 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else if(month.month == 12 && day.day <= 31 && day.day>=1)
        {
            printf("%d,%d,%d",day.day,month.month,year.year);
        }
        else
        {
            printf("invalid date");
        }
    }
}
